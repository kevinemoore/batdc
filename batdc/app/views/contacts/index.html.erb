<h2> Contacts</h2>

<table>
<tr>
<th>Name</th>
<th>Email</th>
<th>Role</th>
<th>School</th>
</tr>
<% @contacts.each do |c| %>
<% name = "#{c.last}, #{c.first}" %>
<tr>
<td><%= link_to name, c %></td>
<td><%= c.email_primary %></td>
<td><%= c.role %></td>
<td><%= c.school.name %></td>
<td><%= button_to 'Edit', edit_contact_path(c), method: :get %>
</tr>
<% end %>
</table>

<%= will_paginate @contacts %>

<%= link_to 'New', new_contact_path() %>
<%= link_to 'Back', contacts_path %>

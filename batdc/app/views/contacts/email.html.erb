<div class="row">
    <%= form_tag(email_contacts_path, :method => "get", role: "search", id: "search-form") do %>
        <%= text_field_tag :search, params[:search], class: "form-control col-md-4 col-sm-4 col-xs-6", placeholder: "Search Contacts" %>
    <% end %>
</div>

<% @contacts.each do |c| %>    
    <div><%= c.email_primary %></div>
<% end %>
